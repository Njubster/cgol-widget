{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "commonjs",
    "checkJs": true,
    "sourceMap": true,
    "outDir": "dist ",
    "rootDir": "./",
    "tsBuildInfoFile": "./",
    "strict": true,
    "strictPropertyInitialization": false,
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}
